{
  "name": "graphql-jay",
  "description": "GraphQL schema generator for composing data over JSON-based services",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "graphql",
    "rest",
    "json",
    "json-rpc",
    "json-schema",
    "json-hyper-schema"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "babel-cli": "^6.14.0",
    "babel-register": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "graphql": "^0.7.0",
    "express": "^4.13.4",
    "express-graphql": "^0.5.4",
    "morgan": "^1.7.0",
    "body-parser": "^1.15.1",
    "cors": "^2.7.1",
    "faker": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "clone": "^1.0.2",
    "deep-assign": "^2.0.0",
    "traverse": "^0.6.6",
    "keypath": "^0.0.1",
    "browserify": "^13.1.0"
  },
  "repository": {
    "type" : "git",
    "url": "http://github.com/mateusmaso/graphql-jay"
  },
  "scripts": {
    "start": "node test/server/index.js",
    "build": "babel src --out-dir lib",
    "build:test": "babel test --out-dir lib/test",
    "pretest": "npm run build",
    "test": "mocha --require babel-register",
    "test:build": "browserify test/index.js -o test/client/index.js",
    "build:browser": "browserify test/index.js -o test/client/index.js"
  },
  "engines": {
    "node" : "~5.9.0"
  }
}
